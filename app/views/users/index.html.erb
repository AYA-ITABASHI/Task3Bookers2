<h1>Users</h1>
<% @users.each do |user| %>
 <div>
  <p>image:
   <%= image_tag user.get_profile_image(100,100) %>
  </p>
  <p>name:<%= user.name %></p>
  <%= link_to"Show", user_path(user.id)%>
<% end %>
 </div>
 <h1>New Book</h1>

  <%= form_with model: @book do |f| %>

    <h4>Title</h4>
      <%= f.text_field :title %>
    <h4>opinion</h4>
      <%= f.text_area :body %>
    <%= f.submit 'create book' %>
  <% end %>


 <%= render 'books/list', books: @books %>
